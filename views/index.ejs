<body class="">
  <%- include('layouts/nav') %>
  <div class="container">
    <% const title = "index" %>
    <div class="col-md-8">
      <div class="row mx-auto">
        <% kosts.forEach(kost => { %>
        <div class="col-md-6 mb-3 mx-auto">
          <div class="card">
            <img
              style="height: 25px"
              src="<%= kost.photos.length ? kost.photos[0].url : '' %> "
              class="card-img-top img-fluid h-25"
              alt="..."
            />
            <div
              class="card-body d-flex justify-content-around align-items-center"
            >
              <h5 class="card-title">Kost <%= kost.name %></h5>
              <a href="#" class="btn btn-primary">Detail</a>
            </div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
  </div>
</body>
