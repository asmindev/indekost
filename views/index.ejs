<body class="">
  <%- include('layouts/nav') %>
  <div class="container">
    <% const title = "index" %>
    <div class="col-md-8">
      <div class="row mx-auto">
        <% kosts.forEach(kost => { %>
        <div class="col-md-6 mb-3 mx-auto">
          <div class="card">
            <img
              style="height: 25px"
              src="<%= kost.photos.length ? kost.photos[0].url : '' %> "
              class="card-img-top img-fluid h-25"
              alt="..."
            />
            <div
              class="card-body d-flex justify-content-around align-items-center"
            >
              <h5 class="card-title">Kost <%= kost.name %></h5>
              <a href="#" class="btn btn-primary">Detail</a>
            </div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
    <div class="p-3">
      <h1>Tambah Data</h1>
      <form
        class="rounded p-2 border"
        action="/upload"
        method="post"
        enctype="multipart/form-data"
      >
        <div class="mb-3">
          <label for="nama" class="form-label">Nama</label>
          <input type="text" class="form-control" id="nama" name="nama" />
        </div>
        <div class="mb-3">
          <label for="file" class="form-label">Photo</label>
          <input type="file" class="form-control" id="file" name="file" accept="image/*" />
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
  </div>
</body>
